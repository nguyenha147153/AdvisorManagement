@model IEnumerable<AdvisorManagement.Models.PlanAdvisor>
@using AdvisorManagement.Models;

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layoutvlu.cshtml";

    List<PlanAdvisor> listYear = (List<PlanAdvisor>)ViewBag.listYear;
    List<VLClass> listYearAdvisor = (List<VLClass>)ViewBag.listYearAdvisor;
    var yearNow = (int)Session["yearNow"];
    var roleABC = (string)Session["role"];
    var yearLast = yearNow - 1;
}
<div class="container-fluid">
    <!-- ============================================================== -->
    <!-- Start Page Content -->
    <!-- ============================================================== -->
    <div class="row">
        <div class="col-12">
            <div class="card">
                <div class="card-body">
                    <h5 class="card-title mb-0">Quản lí kế hoạch năm học</h5>
                </div>
                @if (ViewBag.role == "Admin")
                {
                    <div class="page-breadcrumb">
                        <div class="row">
                            <div class="col-12 d-flex no-block align-items-center">
                                <a onclick="CreateTitle()" id="create" class="btn btn-success" style="margin-bottom: 5px; margin-left: 5px; width: 150px;">
                                    Tạo đề mục<i style="margin-left:5px" class="fas fa-plus"></i>
                                </a>
                                <a href="@Url.Action("PlanSubmited","PlansAdvisor")" class="btn btn-success" style="margin-bottom: 5px; margin-left: 5px; width: 200px;">
                                    Kế hoạch của cố vấn<i style="margin-left:5px" class="fas fa-plus"></i>
                                </a>
                                <a href="@Url.Action("ProofPlan","PlansAdvisor")" class="btn btn-success" style="margin-bottom: 5px; margin-left: 5px; width: 150px;">
                                    Xem minh chứng<i style="margin-left:5px" class="fas fa-plus"></i>
                                </a>
                            </div>
                        </div>
                    </div>
                }

            </div>

            <div class="card">
                <div class="card-body">
                    <h5 class="card-title"></h5>
                    @if (ViewBag.role == "Admin")
                    {
                        <div class="">
                            <label for="cars">Năm học:</label>

                            <select id="semester" name="semester" onchange="myFunction()">
                                <option selected value="@yearNow">@yearLast - @yearNow</option>
                                @foreach (var item in listYear)
                                {
                                    var i = item.year - 1;
                                    <option value="@item.year">@i - @item.year</option>
                                }



                            </select>
                            <a onclick="ExportTemplate()" class="btn btn-success" style=" margin-left: 5px; width: 150px;">
                                Export template <i class="fas fa-plus"></i>
                            </a>
                            <button hidden id="copy" name="copy" class="btn btn-success" style="margin-left: 5px; width: 150px; height: 35px">
                                Copy template <i class="fas fa-copy"></i>
                            </button>
                        </div>
                    }
                    else
                    {
                        <div class="">
                            <label for="cars">Năm học:</label>
                            <select id="year" name="year" onchange="filterFunction()">
                                <option selected value="@yearNow">@yearLast - @yearNow</option>
                                @foreach (var item in listYearAdvisor)
                                {
                                    var i = int.Parse(item.semester_name) - 1;
                                    <option value="@item.semester_name">@i - @item.semester_name</option>
                                }
                            </select>
                        </div>
                    }
                    <div class="table-responsive" style="margin-top:10px">
                        @if (ViewBag.role == "Admin")
                        {


                            <table width="100%" id="zero_config_admin" class="table table-striped table-bordered" role="grid" aria-describedby="zero_config_info">
                                <thead>
                                    <tr>
                                        <th style="text-align:center">STT</th>
                                        <th style="text-align:center">Nội dung</th>
                                        <th style="text-align:center">Mô tả công việc</th>
                                        <th style="text-align:center">Tài nguyên chuẩn bị</th>
                                        <th style="text-align:center">Tiêu chí đánh giá</th>
                                        <th style="text-align:center">Ghi chú</th>
                                        <th hidden>year</th>
                                        <th style="text-align:center" id="action">Hoạt động</th>
                                    </tr>
                                </thead>
                                <tbody id="myTable">
                                </tbody>
                            </table>
                        }
                        else
                        {
                            <table width="100%" id="zero_config_advisor" class="table table-striped table-bordered" role="grid" aria-describedby="zero_config_info">
                                <thead>
                                    <tr>
                                        <th width="10%" style="text-align:center">STT</th>
                                        <th width="25%" style="text-align:center">Mã lớp học</th>
                                        <th width="15%" style="text-align:center">Người cố vấn</th>
                                        <th width="15%" style="text-align:center">Khóa</th>
                                        @*<th width="15%" style="text-align:center">Ngày nhận</th>*@
                                    </tr>
                                </thead>
                                <tbody id="myTable">
                                </tbody>
                            </table>
                        }

                    </div>
                </div>
            </div>
        </div>
    </div>
    <div id="modalCreate" class="modal fade" role="dialog">
        <div class="modal-dialog">

            <!-- Modal content-->
            <div class="modal-content">
                <div class="modal-header">
                    <h4 class="modal-title">Thêm đề mục</h4>
                </div>
                <div class="">
                    <div class="col-md-12" style="padding-bottom: 12px;padding-top: 12px; padding-left:15px">
                        <div class="col-md-4"><span>Số đề mục:</span></div>
                        <div class="col-md-8">
                            <input id="txtIDTitle" type="number" class="form-control" min="1" value="1" />
                        </div>
                    </div>
                </div>
                <div class="">
                    <div class="col-md-12" style="padding-bottom: 12px; padding-top: 12px; padding-left: 15px">
                        <div class="col-md-4"><span>Nội dung:</span></div>
                        <div class="col-md-8">
                            <input id="txtContent" type="text" class="form-control" />
                        </div>
                    </div>
                </div>
                <div class="">
                    <div class="col-md-12" style="padding-bottom: 12px; padding-top: 12px; padding-left: 15px">
                        <div class="col-md-4"><span>Mô tả:</span></div>
                        <div class="col-md-8">
                            <input id="txtDescribe" type="text" class="form-control" />
                        </div>
                    </div>
                </div>
                <div class="">
                    <div class="col-md-12" style="padding-bottom: 12px; padding-top: 12px; padding-left: 15px">
                        <div class="col-md-4"><span>Tài nguyên:</span></div>
                        <div class="col-md-8">
                            <input id="txtSource" type="text" class="form-control" />
                        </div>
                    </div>
                </div>
                <div class="">
                    <div class="col-md-12" style="padding-bottom: 12px; padding-top: 12px; padding-left: 15px">
                        <div class="col-md-4"><span>Tiêu chí đánh giá:</span></div>
                        <div class="col-md-8">
                            <input id="txtEvaluate" type="text" class="form-control" />
                        </div>
                    </div>
                </div>
                <div class="">
                    <div class="col-md-12" style="padding-bottom: 12px; padding-top: 12px; padding-left: 15px">
                        <div class="col-md-4"><span>Ghi chú:</span></div>
                        <div class="col-md-8">
                            <input id="txtNote" type="text" class="form-control" />
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <button onclick="HideModal()" type="button" class="btn btn-default" data-dismiss="modal">Đóng</button>
                    <button id="btnUpload" class="btn btn-primary" type="submit">Thêm</button>
                </div>

            </div>
        </div>
    </div>

    <div id="modalUpdate" class="modal fade" role="dialog">
        <div class="modal-dialog">

            <!-- Modal content-->
            <div class="modal-content">
                <div class="modal-header">
                    <h4 class="modal-title">Chỉnh sửa đề mục</h4>
                </div>
                <input hidden id="id_title" value="" />
                <div class="">
                    <div class="col-md-12" style="padding-bottom: 12px;padding-top: 12px; padding-left:15px">
                        <div class="col-md-4"><span>Số đề mục:</span></div>
                        <div class="col-md-8">
                            <input id="up_Title" type="number" class="form-control" min="1" />
                        </div>
                    </div>
                </div>
                <div class="">
                    <div class="col-md-12" style="padding-bottom: 12px; padding-top: 12px; padding-left: 15px">
                        <div class="col-md-4"><span>Nội dung:</span></div>
                        <div class="col-md-8">
                            <input id="up_Content" type="text" class="form-control" />
                        </div>
                    </div>
                </div>
                <div class="">
                    <div class="col-md-12" style="padding-bottom: 12px; padding-top: 12px; padding-left: 15px">
                        <div class="col-md-4"><span>Mô tả:</span></div>
                        <div class="col-md-8">
                            <input id="up_Describe" type="text" class="form-control" />
                        </div>
                    </div>
                </div>
                <div class="">
                    <div class="col-md-12" style="padding-bottom: 12px; padding-top: 12px; padding-left: 15px">
                        <div class="col-md-4"><span>Tài nguyên:</span></div>
                        <div class="col-md-8">
                            <input id="up_Source" type="text" class="form-control" />
                        </div>
                    </div>
                </div>
                <div class="">
                    <div class="col-md-12" style="padding-bottom: 12px; padding-top: 12px; padding-left: 15px">
                        <div class="col-md-4"><span>Tiêu chí đánh giá:</span></div>
                        <div class="col-md-8">
                            <input id="up_Evaluate" type="text" class="form-control" />
                        </div>
                    </div>
                </div>
                <div class="">
                    <div class="col-md-12" style="padding-bottom: 12px; padding-top: 12px; padding-left: 15px">
                        <div class="col-md-4"><span>Ghi chú:</span></div>
                        <div class="col-md-8">
                            <input id="up_Note" type="text" class="form-control" />
                        </div>
                    </div>
                </div>



                <div class="modal-footer">
                    <button onclick="HideModalUp()" type="button" class="btn btn-default" data-dismiss="modal">Đóng</button>
                    <button id="btnEdit" class="btn btn-primary" type="submit">Sửa</button>
                </div>

            </div>
        </div>
    </div>

