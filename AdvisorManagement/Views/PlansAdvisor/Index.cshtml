<script src="~/Assets/extra-libs/DataTables/datatables.js"></script>
<script src="~/Assets/extra-libs/DataTables/datatables.min.js"></script>
<script type="text/javascript" src="https://cdn.rawgit.com/ashl1/datatables-rowsgroup/fbd569b8768155c7a9a62568e66a64115887d7d0/dataTables.rowsGroup.js"></script>
<script>
        //Lấy data khi vừa vào web admin
         var dataTable = $("#zero_config_admin").DataTable({
         'ajax': {
                'url': '@Url.Action("GetData", "PlansAdvisor")',
                'data': function (d) {
                 var input = document.getElementById("semester");
                 filter = input.value.toUpperCase();
                 d.year = filter ;
                 },
                'type': 'get',
                'datatype':'json',
         },
         'columns':
             [
                 { 'data': 'number_title', 'width': '0%' },
                 { 'data': 'content', 'width': '20%' },
                 { 'data': 'describe', 'width': '25%' },
                 { 'data': 'source', 'width': '25%' },
                 { 'data': 'evaluate', 'width': '20%' },
                 { 'data': 'note', 'width': '5%' },                
                 {
                     'data': 'id', 'width': '20%', 'render': function (data) {
                         return '<button onclick="Update(' + data + ')" class="btn btn-info">Sửa <i class="fas fa-tasks" ></i></button > <button onclick="Delete(' + data + ')" class="btn btn-danger" >Xóa <i class="fas fa-trash-alt" ></i ></button >@* <a id="proof" href="@Url.Action("Details", "PlansAdvisor")/' + data + '/" class="btn btn-info">Minh chứng <i class="fas fa-edit"></i></a>*@'
                     }
                 },
                 ],
             'bAutoWidth': false, 
             'responsive': false,
             "order": [[0, "asc"]],
             'ordering': false,
             'rowsGroup': [0,1],           
             'language': {
                 "decimal": "",
                 "emptyTable": "Không có dữ liệu",
                 "info": "Hiển thị từ _START_ đến _END_ trong _TOTAL_ mục",
                 "infoEmpty": "Showing 0 to 0 of 0 entries",
                 "infoFiltered": "(filtered from _MAX_ total entries)",
                 "infoPostFix": "",
                 "thousands": ",",
                 "lengthMenu": "Hiển thị _MENU_ mục",
                 "loadingRecords": "Đang tải...",
                 "search": "Tìm kiếm:",
                 "zeroRecords": "No matching records found",
                 "paginate": {
                     "first": "First",
                     "last": "Last",
                     "next": "Sau",
                     "previous": "Trước"
                 },                 
             }
         });      

        $(document).ready(function () {
        if ("@roleABC" == "Advisor") {
         $("#zero_config_admin").dataTable().fnDestroy();
         var dataTable = $("#zero_config_advisor").DataTable({
         'ajax': {
                'url': '@Url.Action("GetDataAdvisor", "PlansAdvisor")',
                'data': function (d) {
                 /*var input = document.getElementById("semester");
                 filter = input.value.toUpperCase();*/
                 d.year = @yearNow ;
                 },
                'type': 'get',
                'datatype':'json',
         },
         'columns':
             [
                 { 'data': 'ID', 'width': '5%', },
                 {
                     'data': 'idClass', 'width': '25%', 'render': function (data, type, row) {
                             return '<a id="proof" href="@Url.Action("DetailClass", "PlansAdvisor")/' + row.ID + '/" >' + data + '</a>';
                     }
                 },
                 { 'data': 'name', 'width': '25%' },
                 { 'data': 'course', 'width': '30%' },
                 ],

         });
            dataTable.on('order.dt search.dt', function () {
                let i = 1;
                dataTable.cells(null, 0, { search: 'applied', order: 'applied' }).every(function (cell) {
                    this.data(i++);
                });
            }).draw();
        }
        });

        //filter data khi Admin chọn năm-->-->-->-->
        function myFunction() {
            var input = document.getElementById("semester");
            filter = input.value.toUpperCase();
            if (filter != @yearNow) {
                seleclYear()
            } else {
                selectYearNow()
            }
        }
        function seleclYear() {
            $("#zero_config_admin").dataTable().fnDestroy();
            $("#zero_config_admin").DataTable({
             'ajax': {
                    'url': '@Url.Action("GetData", "PlansAdvisor")',
                    'data': function (d) {
                     var input = document.getElementById("semester");
                        filter = input.value.toUpperCase();
                     d.year = filter ;
                     },
                    'type': 'get',
                    'datatype':'json',
                 },
             'columns':
                 [
                     { 'data': 'number_title', 'width': '5%' },
                     { 'data': 'content', 'width': '25%' },
                     { 'data': 'describe', 'width': '30%' },
                     { 'data': 'source', 'width': '25%' },
                     { 'data': 'note', 'width': '30%' },
                     {
                         'data': 'id', 'visible': false, 'render': function (data) {
                             document.getElementById("copy").hidden = false;
                             document.getElementById("action").hidden = true;
                                 return '<a id="proof" href="@Url.Action("Details", "PlansAdvisor")/'+ data +'/" class="btn btn-info">Minh chứng <i class="fas fa-edit"></i></a>'
                         }
                     },
                     ],
             });
        }
        function selectYearNow() {
            $("#zero_config_admin").dataTable().fnDestroy();
            $("#zero_config_admin").DataTable({
             'ajax': {
                    'url': '@Url.Action("GetData", "PlansAdvisor")',
                    'data': function (d) {
                     var input = document.getElementById("semester");
                     filter = input.value.toUpperCase();
                     d.year = filter ;
                     },
                    'type': 'get',
                    'datatype':'json',
             },
             'columns':
                 [
                     { 'data': 'number_title', 'width': '5%' },
                     { 'data': 'content', 'width': '25%' },
                     { 'data': 'describe', 'width': '30%' },
                     { 'data': 'source', 'width': '25%' },
                     { 'data': 'note', 'width': '10%' },
                     {
                         'data': 'id', 'width': '30%', 'render': function (data) {
                             document.getElementById("action").hidden = false;
                             document.getElementById("copy").hidden = true;
                             return '<button onclick="Update(' + data + ')" class="btn btn-info">Sửa <i class="fas fa-tasks" ></i></button > <button onclick="Delete(' + data +')" class="btn btn-danger" >Xóa <i class="fas fa-trash-alt" ></i ></button > @*<a id="proof" href="@Url.Action("Details", "PlansAdvisor")/'+ data +'/" class="btn btn-info">Minh chứng <i class="fas fa-edit"></i></a>*@'
                         }
                     },
                 ]
            });
    }

        function filterFunction() {
            $("#zero_config_advisor").dataTable().fnDestroy();
            var dataTable = $("#zero_config_advisor").DataTable({
                 'ajax': {
                        'url': '@Url.Action("GetDataAdvisor", "PlansAdvisor")',
                        'data': function (d) {
                         var input = document.getElementById("year");
                         filter = input.value.toUpperCase();
                            d.year = filter ;
                         },
                        'type': 'get',
                        'datatype':'json',
                 },
                 'columns':
                     [
                         { 'data': 'ID', 'width': '5%', },
                         {
                             'data': 'idClass', 'width': '25%', 'render': function (data, type, row) {
                                     return '<a id="proof" href="@Url.Action("DetailClass", "PlansAdvisor")/' + row.ID + '/" >' + data + '</a>';
                             }
                         },
                         { 'data': 'name', 'width': '25%' },
                         { 'data': 'course', 'width': '30%' },
                         ],

                 });
            dataTable.on('order.dt search.dt', function () {
                let i = 1;
                dataTable.cells(null, 0, { search: 'applied', order: 'applied' }).every(function (cell) {
                    this.data(i++);
                });
            }).draw();
        }

        $(document).ready(function () {
            $("#myInput").on("keyup", function () {
                var value = $(this).val().toLowerCase();
                $("#myTable tr").filter(function () {
                    $(this).toggle($(this).text().toLowerCase().indexOf(value) > -1)
                });
            });
        });

        function CreateTitle() {
            $("#modalCreate").modal("show");
        }

        function CopyTemplate() {
            $("#modalCopy").modal("show");
        }

        function HideModal() {
            $("#modalCreate").modal("hide");
    }

        //Lấy thông tin người dùng để cập nhật
         function Update(id) {
              $.ajax({
                    url:'@Url.Action("Update", "PlansAdvisor")',
                    type: 'get',
                    data: {
                        id: id
                    },
                    success: function (data) {
                        if (data.success == true) {
                            $('#up_Title').val(data.T.number_title);
                            $('#up_Content').val(data.T.content);
                            $('#up_Describe').val(data.T.describe);
                            $('#up_Source').val(data.T.source);
                            $('#up_Note').val(data.T.note);
                            $('#up_Evaluate').val(data.T.evaluate);                            
                            $('#id_title').val(data.T.id);
                            $('#modalUpdate').modal("show");
                        } else {
                            alert(data.message);
                        }
                    }
                })
         }

         //Cập nhật thông tin người dùng
        $('#btnEdit').click(function () {
                let id = $('#id_title').val();
            let numTitle = $('#up_Title').val();
            if (numTitle != "") {
                let content = $('#up_Content').val();
                let describe = $('#up_Describe').val();
                let source = $('#up_Source').val();
                let note = $('#up_Note').val();
                let evaluate = $('#up_Evaluate').val();   
                $.ajax({
                    type: 'POST',
                    url: '@Url.Action("UpdateTittle", "PlansAdvisor")',
                    data: {
                        id: id,
                        numTitle: numTitle,
                        content: content,
                        describe: describe,
                        source: source,
                        note: note,
                        evaluate: evaluate
                    },
                    success: function (data) {
                        if (data.success == true) {
                            alert(data.message);
                            $('#modalUpdate').modal('hide');
                            dataTable.ajax.reload(null, false);
                        } else {
                            alert(data.message);
                        }
                    }
                })
            } else {
                alert("Vui lòng nhập đầy đủ trường dữ liệu");
            }
            })

        function HideModalUp() {
            $("#modalUpdate").modal("hide");
            $("#modalAssignment").modal("hide");
        }

        function HideModalCopy() {
            $("#modalCopy").modal("hide");
        }


         $(document).ready(function () {
        $('#btnUpload').click(function () {
            var formData = new FormData();
            var idtitle = $("#txtIDTitle").val();
            var content = $("#txtContent").val();
            var describe = $("#txtDescribe").val();
            var source = $("#txtSource").val();
            var note = $("#txtNote").val();
            var evaluate = $("#txtEvaluate").val();
            if (idtitle != "") {
                formData.append("idtitle", idtitle);
                formData.append("content", content);
                formData.append("describe", describe);
                formData.append("source", source);
                formData.append("note", note);
                formData.append("evaluate", evaluate);
                $.ajax({
                type: 'POST',
                url: '@Url.Action("CreateTitle", "PlansAdvisor")',
                contentType: false,
                processData: false,
                data: formData,
                success: function (data) {
                    if (data.success == false) {
                        alert(data.message);
                    } else {
                        $("#txtIDTitle").val("1");
                        $("#txtContent").val("");
                        $("#txtDescribe").val("");
                        $("#txtSource").val("");
                        $("#txtNote").val("");
                        $("#txtEvaluate").val("");   
                        $("#modalCreate").modal("hide");
                        alert(data.message);
                        dataTable.ajax.reload(null, false);
                    }
                }
             })
            } else {
                alert("Vui lòng nhập đầy đủ trường dữ liệu");
            }
        })
         })

         //Sự kiện xóa
         function Delete(id) {
             if (confirm("Bạn thực sự muốn xóa đề mục này?")) {
                $.ajax({
                    url: '@Url.Action("Delete", "PlansAdvisor")',
                    type: 'POST',
                    data: {
                        id: id
                    },
                    success: function (data) {
                         if (data.success == true) {
                             alert(data.message);
                             dataTable.ajax.reload(null, false);
                         } else {
                             alert(data.message);
                         }
                    }
                })
            }
         }

        //Copy template
        $(document).on('click', "button[name=copy]", function () {
            input = document.getElementById("semester");
            let year = input.value.toUpperCase();
            if (confirm("Bạn muốn sao chép template năm " + year +"?")) {
            $.ajax({
                url: '@Url.Action("Copy", "PlansAdvisor")',
                type: 'POST',
                data: {
                    id: year
                },
                success: function (data) {
                     if (data.success == true) {
                         alert(data.message);
                         location.reload();
                     } else {
                         alert(data.message);
                     }
                }
            })
        }
        })

        //Export template
        function ExportTemplate() {
            input = document.getElementById("semester");
            let id = input.value.toUpperCase();
            $.ajax({
                url: '@Url.Action("ExportTemplate", "PlansAdvisor")',
                type: 'POST',
                data: {
                    year: id
                },
                /*contentType: "application/json; charset=utf-8", */
                success: function (data) {
                    var bytes = new Uint8Array(data.FileContents);
                    var blob = new Blob([bytes], { type: 'application/vnd.openxmlformats-officedocument.spreadsheetml.sheet' });
                    var downloadUrl = URL.createObjectURL(blob);
                    var a = document.createElement("a");
                    a.href = downloadUrl;
                    a.download = data.FileDownloadName;
                    document.body.appendChild(a);
                    a.click();
                    document.body.removeChild(a);
                },
                error: function (err) {
                    alert(err);
                },
            })
        }

